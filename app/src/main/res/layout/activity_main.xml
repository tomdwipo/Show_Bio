<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".MainActivity">



    <ImageView
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:id="@+id/gambarSatu"
        android:layout_centerHorizontal="true"
        android:background="@drawable/satu"/>

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:id="@+id/gambarDua"
        android:background="@drawable/dua"
        android:layout_above="@+id/ditemukanDua"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Ditemukan satu"
        android:id="@+id/ditemukanSatu"
        android:layout_below="@+id/gambarSatu"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Ditemukan dua"
        android:id="@+id/ditemukanDua"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/ditemukanSatu"
        android:layout_alignStart="@+id/ditemukanSatu" />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/posisiList"
        android:text="listView"
        android:layout_below="@+id/gambarSatu"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/Listview22"
        android:text="listView2"
        android:layout_below="@+id/gambarSatu"
        android:layout_alignParentRight="true"/>

</RelativeLayout>
